        "jsreport-chrome-pdf": "^1.1.5",
        "jsreport-core": "^2.2.0",
        "jsreport-handlebars": "^2.0.0",
        "jsreport-html-embedded-in-docx": "^1.0.0",
        "log4js": "^3.0.5",
        "moment": "^2.22.2",
        "mongoose": "^5.3.0",
        "puppeteer": "^1.8.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
